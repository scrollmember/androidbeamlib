		<item name="android:layout_marginTop">30dip</item>
	</style>
	<style name="menu_button_right_style" parent="menu_layout_style">
		<item name="android:layout_marginLeft">25dip</item>
		<item name="android:layout_marginRight">30dip</item>
		<item name="android:layout_marginTop">30dip</item>
	</style>
	<style name="menu_text_left_style" parent="menu_layout_style">
		<item name="android:layout_marginLeft">30dip</item>
		<item name="android:layout_marginRight">25dip</item>
		<item name="android:layout_marginBottom">15dip</item>
		<item name="android:textSize">16.0dip</item>
		<item name="android:textColor">#000000</item>
		<item name="android:textColorLink">#000000</item>
	</style>
	<style name="menu_text_right_style" parent="menu_layout_style">
